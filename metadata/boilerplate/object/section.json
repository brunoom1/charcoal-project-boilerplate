{
  "properties":{
    "master":{
      "obj_type":"boilerplate/object/section"
    }
  },

  "sources":{
    "default":{
      "table":"boilerplate_sections"
    }
  },
  "default_source":"default",

  "admin":{
    "lists":{
      "default":{
        "properties":[
          "title"
        ]
      }
    },
    "default_list":"default",

    "forms":{
      "cms.default": {
        "groups": {
          "cms.info": {
            "priority": 100
          },
          "content-blocks": {
            "type": "charcoal/admin/widget/form-group/attachment",
            "preset": "content-blocks",
            "priority": 200
          },
          "cms.advanced": {
            "priority": 300
          },
          "cms.dev": {
            "priority": 350
          },
          "cms.meta": {
            "priority": 400
          },
          "cms.table": {
            "priority": 7000
          },
          "cms.revisions": {
            "priority": 8000
          }
        },
        "sidebars": {
          "default": {
            "properties": [
              "created",
              "created_by",
              "last_modified",
              "last_modified_by"
            ]
          }
        }
      }
    },
    "default_form":"cms.default",

    "dashboards": {
      "admin.edit": {
        "layout": {
          "structure": [
            {"columns": [1]}
          ]
        },
        "widgets": {
          "form": {
            "type": "charcoal/admin/widget/objectForm",
            "form_ident": "cms.default",
            "group_display_mode": "tab"
          }
        }
      },
      "admin.table": {
        "layout": {
          "structure": [
            {"columns": [1]}
          ]
        },
        "widgets": {
          "table": {
            "type": "charcoal/admin/widget/table",
            "obj_type":"boilerplate/object/section",
            "collection_ident": "cms.default"
          }
        }
      }
    },
    "default_create_dashboard": "admin.edit",
    "default_edit_dashboard": "admin.edit",
    "default_collection_dashboard": "admin.table"
  }
}